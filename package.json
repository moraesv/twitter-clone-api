{
  "name": "twitter-clone-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/moraesv/twitter-clone-api.git",
  "author": "Vinicius de Moraes <vimoraescarvalho@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "yarn eslint --fix ./src",
    "typeorm": "env-cmd -f ./.env node --require ts-node/register ./node_modules/typeorm/cli.js",
    "start": "env-cmd -f ./.env ts-node src/server.ts",
    "dev": "env-cmd  -f ./.env ts-node-dev --ignore-watch node_modules src/server.ts",
    "db:recreate": "yarn db:drop && yarn db:create && yarn migration:run",
    "db:create": "env-cmd -f ./.env pg-god db-create",
    "db:drop": "env-cmd -f ./.env pg-god db-drop",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:show": "yarn typeorm migration:show"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.14",
    "@types/morgan": "^1.9.1",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jest": "^26.5.3",
    "morgan": "^1.10.0",
    "mysql": "^2.14.1",
    "pg": "^8.3.0",
    "pg-god": "^1.0.11",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.25"
  }
}
